{
  "name": "lvlart-api",
  "version": "1.1.0",
  "description": "lvl-art api",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/erickangweisz/lvl-art-api.git"
  },
  "keywords": [
    "lvl-art",
    "lvlart"
  ],
  "author": "jonatan arrocha kang",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/erickangweisz/lvl-art-api/issues"
  },
  "homepage": "https://gitlab.com/erickangweisz/lvl-art-api#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "connect-multiparty": "^2.2.0",
    "email-existence": "^0.1.6",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jwt-simple": "^0.5.6",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "mongoose-pagination": "^1.0.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
